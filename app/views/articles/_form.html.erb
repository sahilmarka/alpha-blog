<%= form_with(model:@article, local:true) do |f| %>
    <div>
        <%= f.label :title %><br>
        <%= f.text_field :title%><br>
    </div>
    </div>
        <%= f.label :description %><br>
        <%= f.text_area :description%><br>
    </div>
    <div>
        <%= f.submit %>
    </div>
<% end %>